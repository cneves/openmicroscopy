<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<css>

	<selector name="body">
		<attrib name="background-color" value="white"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="12px"/>
		<attrib name="margin-top" value="10px"/>
		<attrib name="margin-left" value="0px"/>
	</selector>

	<selector name="a">
		<attrib name="text-decoration" value="none"/>
		<attrib name="color" value="steelblue"/>
	</selector>
	
	<selector name="td">
		<attrib name="font-size" value="12px"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
	</selector>

	<selector name="p">
		<attrib name="font-size" value="12px"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
	</selector>
	
	<selector name="navigation">
		<attrib name="width" value="100%"/>
		<attrib name="height" value="15"/>
		<attrib name="background-color" value="#F0F0F0"/>
		<attrib name="color" value="#000000"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="10px"/>
		<attrib name="font-weight" value="bold"/>
	</selector>
	
	<selector name="section-heading-1">
		<attrib name="color" value="#000000"/>
		<attrib name="text-align" value="left"/>
		<attrib name="vertical-align" value="bottom"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="20px"/>
		<attrib name="font-weight" value="bold"/>
	</selector>
	
	<selector name="section-heading-2">
		<attrib name="color" value="#000000"/>
		<attrib name="text-align" value="left"/>
		<attrib name="vertical-align" value="bottom"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="14px"/>
		<attrib name="font-weight" value="bold"/>
	</selector>

	<selector name="section-body-1">
		<attrib name="margin-left" value="10px"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
	</selector>
	
	<selector name="section-body-1">
		<attrib name="margin-left" value="10px"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
	</selector>
	
	<selector name="section-body-2">
		<attrib name="margin-left" value="10px"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
	</selector>
	
	<selector name="document-footer">
		<attrib name="text-align" value="right"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="8px"/>
	</selector>
	
	<selector name="document-header">
		<attrib name="color" value="#000000"/>
		<attrib name="text-align" value="right"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="16px"/>
		<attrib name="font-weight" value="bold"/>
	</selector>
	
	<selector name="document-title">
		<attrib name="color" value="gray"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="22px"/>
		<attrib name="font-weight" value="bold"/>
	</selector>
	
	<selector name="document-attributes">
		<attrib name="color" value="#000000"/>
		<attrib name="text-align" value="left"/>
		<attrib name="font-family" value="Verdana,Arial,Helvetica"/>
		<attrib name="font-size" value="12px"/>
	</selector>
	
	<selector name="note">
		<attrib name="border" value="1px"/>
		<attrib name="border-style" value="solid"/>
		<attrib name="padding" value="1px"/>
		<attrib name="background-color" value="#F0F0F0"/>
		<attrib name="color" value="#000000"/>
		<attrib name="font-size" value="10px"/>
	</selector>
	
</css>